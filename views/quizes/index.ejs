<% include search.ejs %>
<% if (quizes.length) { %>
<table>
	<% var i; for (i=0; i < quizes.length; i++) { %>        
	<tr><td><a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td></tr> 
	<% } %>
</table>
<% } else { %>
<h2>No hay preguntas que coincidan con su busqueda.</h2>
<% } %>

